{
    "name": "celysium/router",
    "description": "Get all api routes , get their paths and convert them to an object ",
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Celysium\\Router\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ehsan Tavakoli",
            "email": "ehsantavakoli1995@gmail.com"
        },
        {
            "name" : "Nasser Hekmati",
            "email" : "hekmati.nasser@gmail.com"
        }
    ],
    "require": {
        "php" : "^8.0",
        "illuminate/support" : "^8.0|^9.0",
        "illuminate/routing" : "^8.0|^9.0"
    },
    "keywords": [
        "Laravel",
        "api",
        "postman",
        "swagger",
        "collection",
        "generate"
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Celysium\\Router\\RouterServiceProvider"
            ],
            "aliases": {
                "router": "Celysium\\Router\\Router"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
